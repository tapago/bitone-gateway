{
    "name": "bitone/gateway",
    "description": "Gateway de pagamentos completo e transparentepara o seu negócio crescer de forma estruturada.",
    "keywords": ["bit", "bitone", "gateway", "sdk"],
    "type": "library",
    "license": "MIT",
    "version": "1.0",
    "authors": [{
        "name": "Éderson Sandre",
        "email": "ederson.sandre@gmail.com"
    }],
    "require": {
        "php": ">=5.4",
        "rmccue/requests": ">=1.0",
        "zendframework/zend-filter": "^2.7"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.4",
        "phpunit/php-code-coverage": "^4"
    },
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "BitOne\\Gateway\\": [
                "src/BitOne/Gateway",
                "tests/BitOne/Gateway"
            ]
        }
    },
    "config":{
        "bin-dir":"bin/"
    }
}
